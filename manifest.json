{
    "name": "Up Index Q",
    "description": "Chrome extension to show Bilibili view count / likes ratio",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {
        "16": "./icon.png",
        "32": "./icon.png",
        "48": "./icon.png",
        "128": "./icon.png"
    },
    "background": {
        "scripts": ["./background.js"]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": ["tabs", "https://www.bilibili.com/"]
}
